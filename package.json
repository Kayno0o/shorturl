{
  "name": "shorturl",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "bunx --bun astro dev --port 4192 --host 127.0.0.1",
    "build": "bunx --bun astro build --port 4192 --host 127.0.0.1",
    "preview": "bunx --bun astro preview --port 4192 --host 127.0.0.1",
    "astro": "bunx --bun astro",
    "api:dev": "bun run --watch ./src/api/index.ts --port 4193",
    "lint:fix": "bunx --bun eslint --fix"
  },
  "dependencies": {
    "@astrojs/node": "^9.2.2",
    "@kaynooo/ts-module": "^1.0.14",
    "@kaynooo/utils": "^1.2.15",
    "astro": "^5.8.2",
    "elysia": "^1.3.4",
    "jsonwebtoken": "^9.0.2",
    "xxhash-wasm": "^1.1.0"
  },
  "devDependencies": {
    "@kaynooo/eslint": "^1.1.6",
    "@types/bun": "^1.2.15",
    "@types/jsonwebtoken": "^9.0.9",
    "eslint": "^9.28.0",
    "eslint-plugin-astro": "^1.3.1"
  }
}
